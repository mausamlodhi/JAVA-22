Q13.
class Test
{
    public static void main (String[] args)
    {
        int i ,j,k=1;
        for (i =1 ; i<=4 ; i++)
        {
            for (j=1;j<=2*i-1 ;j++)
            {
                if (j%2==0)
                    System.out.print("*");
                else if (j%2==1)
                {
                    System.out.print(k);
                    k++;
                }
                else
                    System.out.print("  ");
            }
            System.out.println();
            }
        for (i=4 ;i>=1 ; i--)
        {
            for (j=1 ;j<=2*i-1 ;j++)
            {
                if (j%2==0)
                    System.out.print("*");
                else if (j%2==1)
                {
                    System.out.print(k);
                    k--;
                }
                else
                  System.out.print(" ");
            }
            System.out.println();
        }
    }
}

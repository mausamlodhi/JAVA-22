Q16. 
public class trick
{
    public static void main(String[] args)
    {
        int Row = 1, c = 1;
        for (int i = 1; i <= 5; i++)
        {
            if (i % 2 == 0)
            {
                int reverse = Row + c - 1;
                for (int j = 0; j < i; j++)
                {
                    System.out.print(reverse-- + " ");
                    c++;
                }
            }
            else
            {
                for (int j = 1; j <= i; j++)
                {
                    System.out.print(c + " ");
                    c++;
                }
            }
            System.out.println();
            Row++;
        }
    }
}
*********************************************************************
Q16.
class Test
{
    public static void main(String[] args)
    {
       int n =2,k,primeCount=0;
        for (int i = 1; i <=5; i++)
        {
            primeCount=0;
           
               while(true)
               {
                 k=2;
                 while(k<=n/2)
                 {
                   if(n%k==0)
                     break;
                  k++;
                }
                if(k>n/2)
                  {
                    primeCount++;
                    System.out.print(n+" ");
                  }
                 n++;
                
                 if(primeCount == i){
                   System.out.println();
                   break;
                 }  
                   
               }     
           
        }

    }
}

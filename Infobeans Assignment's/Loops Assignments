Q10.
public class Practice
{
    public static void main(String[] args)
    {
        for(int i=1;i<=5;i++)
        {
            for(int j=6;j>=i;j--)
                System.out.print("*");
            for(int k=1;k<=i+i;k++)
            {
                if(k%2==0)
                    System.out.print("*");
                else
                    System.out.print(i);
            }
            for(int j=5;j>=i;j--)
                System.out.print("*");
            System.out.println();
        }
    }
}

Q4.
class Test
 {
   public static void main(String args[])
    {
     int i,j,k,s;
      for(i=1;i<=5;i++)
        {
          for(j=5;j>i;j--)
            System.out.print(" ");
          for(k=i,s=1;s<=2*i-1;s++)
           {
              if(s<=i)
               {
                 System.out.print(k);
                 k++;
                 if(s==i)
                   --k;
               }
               else
                 {
                   --k;
                   System.out.print(k);
                 }
          }
          System.out.println();
       }
    }
}

Q1. Hollow square star pattern???
class TestMain
{
  public static void main(String args[])
    {
      for(int i=0;i<=9;i++)
     	{
     	   for(int j=0;j<=9;j++)
     	   {
     	      if(j==i||j==9-i||i==0||i==9||j==0||j==9)
     	        System.out.print("*");
     	      else 
     	         System.out.print(" ");
     	    }
     	  System.out.println();
     	 }
     }
}

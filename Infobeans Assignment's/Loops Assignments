Q13
class Test
{
    public static void main(String[] args)
    {
      int num = 8,i, j , k, t1, t2,var = 1;
         for (i = 0; i <4; i++)
          {
            for (j = 0; j <= i * 2; j++)
              {
                if (j % 2 == 0)
                  {
                   System.out.print(+var);
                   var++;
                  }
                else
                  System.out.print("*");
              }
                System.out.println();
          }
                var--;
                j = (i - 1) * 2;
                var = var - (i - 1);
                k = (num / 2) - 1;
                for (i = num / 2; i < num; i++)
                {
                    t1 = j;
                    t2 = var;
                    while (j >= 0)
                    {
                        if (j % 2 == 0)
                        {
                            System.out.print(+var);
                            var++;
                        } else
                            System.out.print("*");
                        j--;
                    }
                    System.out.println();
                    j = t1 - 2;
                    var = t2 - k;
                    k--;
                }
        }
  }

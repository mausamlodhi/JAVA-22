Q2 WAP to print Hollow Inverted Mirrored right triangle pattern star??
class TestMain
{
  public static void main(String args[])
    {
      for(int i=0;i<=9;i++)
     	{
     	   for(int j=0;j<=9;j++)
     	   {
     	   	  if(i==j||j==9||i==0)
     	   	    System.out.print("*");
     	   	  else
     	   	     System.out.print(" ");
     	   }
     	   System.out.println();
        }
     }
}

class Test
 {
   public static void main(String args[])
    {
      for(int i=1;i<=9;i++)
       {
         for(int j=9;j>=9-i;j--)
             System.out.print("  ");
         for(int k=1;k<=9-i;k++)
             System.out.print(" *  ");
        System.out.println();
       }
    }
}

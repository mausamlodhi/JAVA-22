Q8.
class Test
{
    public static void main(String[] args)
    {
        for (int i = 65; i <=69; i++)
        {
            for (int j = 65; j <= i; j++)
            {
                System.out.print((char)j + " ");
            }
            for (int k = i - 1; k >= 69; k--)
            {
                System.out.print((char)k + " ");
            }
                System.out.println();
        }
    }
}

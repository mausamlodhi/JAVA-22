Q9.WAP to print Diamond star pattern????

class Test{
    public static void main(String[] args)
    {
        for (int i = 1; i <=9; i++)
        {
            for(int j=9;j>=i+1;j--)
                System.out.print("  ");
            for(int k=1;k<=i;k++)
                System.out.print("  * ");
            System.out.println();
        }
         for (int i = 1; i <=8; i++)
        {
            for(int j=1;j<=i;j++)
                System.out.print("  ");
            for(int k=8;k>=i;k--)
                System.out.print("  * ");
            System.out.println();
        }
    }
}
